CREATE OR REPLACE TABLE
    exercise_db.public.customer_address_sample_5 
AS
    SELECT 
        *
    FROM 
        "SNOWFLAKE_SAMPLE_DATA"."TPCDS_SF10TCL"."CUSTOMER_ADDRESS"
    SAMPLE
        ROW(1)
        SEED(2);

CREATE OR REPLACE TABLE
    exercise_db.public.CUSTOMER_SAMPLE_1 
AS
    SELECT 
        *
    FROM 
        "SNOWFLAKE_SAMPLE_DATA"."TPCDS_SF10TCL"."CUSTOMER"
    SAMPLE
        SYSTEM(1)
        SEED(2);

SELECT
    COUNT(*)
FROM 
    exercise_db.public.CUSTOMER_SAMPLE_1;
